{
  "name": "desafionodejs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node dist/index",
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "dev:server": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/index.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:generate ./src/migrations/default",
    "migration:run": "npm run typeorm migration:run -- -t=each -d src/data-source.ts",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js"
  },
  "author": "Giuliano Mitteroheffer Morelli",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.4.0",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^2.0.0",
    "typeorm": "0.3.10"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.11.10",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  }
}
